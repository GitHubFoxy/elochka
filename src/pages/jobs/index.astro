<Layout page="4">
    <main class="pt-[133px]" >
        <section class=" bg-orange-600 w-full h-52 grid place-content-center">
            <h1 class="text-center text-white text-4xl font-bold uppercase" >ВАКАНСИИ</h1>
        </section>
        <section class="py-12">
            <h1 class="uppercase text-center text-4xl font-bold mb-10" >ОТКРЫТО 3 ВАКАНСИИ:</h1>
            <div class="container mx-auto flex flex-col gap-12">
                <div class="flex gap-12">
                    <div>
                        <div class="flex">
                            <div>
                                <svg width="60" height="60" viewBox="0 0 60 60" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                <rect width="60" height="60" fill="url(#pattern0_17_3)"/>
                                <defs>
                                <pattern id="pattern0_17_3" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_17_3" transform="scale(0.00195312)"/>
